[Unit]
Description=gunicorn daemon
After=network.target

[Service]
User=nodia
Group=www-data
WorkingDirectory=/home/nodia/RKK_Backend
ExecStart=/home/nodia/RKK_Backend/env/bin/gunicorn --access-logfile - --workers 3 --bind unix:/run/gunicorn.sock nodia.wsgi:application

[Install]
WantedBy=multi-user.target
